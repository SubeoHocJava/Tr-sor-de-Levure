@model IEnumerable<dynamic>
<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>Bộ Sưu Tập Rượu Whisky Thượng Hạng</title>
    <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.3.3/dist/css/bootstrap.min.css"
          rel="stylesheet"
          integrity="sha384-QWTKZyjpPEjISv5WaRU9OFeRpok6YctnYmDr5pNlyT2bRjXh0JMhjY6hW+ALEwIH"
          crossorigin="anonymous" />
    <link href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/5.15.4/css/all.min.css"
          rel="stylesheet" />

    <link rel="stylesheet"
          href="https://cdnjs.cloudflare.com/ajax/libs/noUiSlider/15.6.1/nouislider.min.css" />
    <link rel="stylesheet" href="~/css/Whisky.css" />
</head>
<body>
    <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.3.3/dist/js/bootstrap.bundle.min.js"></script>
    <!--intro whisky page-->
    <div id="carouselIntroWhisky"
         class="carousel intro"
         data-bs-ride="carousel">
        <div class="carousel-inner">
            <div class="carousel-item active">
                <img src="https://whisky.vn/wp-content/uploads/2024/07/banner-whisky-GrandCrus.jpg"
                     class="d-block w-100"
                     alt="whiskyIntro1" />
            </div>
            <div class="carousel-item">
                <img src="https://whisky.vn/wp-content/uploads/2024/05/Hang-hiem.jpg"
                     class="d-block w-100"
                     alt="whiskyIntro2" />
            </div>
            <div class="carousel-item">
                <img src="https://whisky.vn/wp-content/uploads/2024/07/Banner-Rum.png"
                     class="d-block w-100"
                     alt="whiskyIntro3" />
            </div>
        </div>
    </div>

    <!--grid page -->
    @RenderBody()
    <script src="https://code.jquery.com/jquery-3.6.4.min.js"></script>
    <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.3.3/dist/js/bootstrap.bundle.min.js"></script>
    <script src="@Url.Content("~/js/whisky.js")"></script>

</body>
</html>
